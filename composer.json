{
  "name": "symphonic-emotions/php-midi",
  "description": "PHP library to parse MIDI files (tempo, time signature, etc.)",
  "type": "library",
  "license": "WTFPL",
  "autoload": {
    "psr-4": {
      "symphonic-emotions\\PhpMidi\\": "src/"
    },
    "classmap": [
      "src/midi.class.php",
      "src/midi_duration.class.php",
      "src/midi_conversion.class.php",
      "src/midi_trim.class.php",
      "src/midi_volume.class.php"
    ]
  },
  "require": {
    "php": ">=8.0"
  },
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Frans-Jan Wind",
      "email": "frans-jan@symphonic-emotions.nl"
    }
  ]
}
